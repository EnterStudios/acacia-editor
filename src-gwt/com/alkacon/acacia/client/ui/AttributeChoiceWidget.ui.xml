<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:aui="urn:import:com.alkacon.geranium.client.ui">
	<ui:with field="style" type="com.alkacon.acacia.client.css.I_LayoutBundle" />
	<g:HTMLPanel styleName="{style.attributeChoice.attributeChoice}">
		<span class="{style.attributeChoice.buttonIcon}" ui:field="m_buttonIcon"></span>
		<div class="{style.attributeChoice.frame}">
			<div class="{style.attributeChoice.bridge}"></div>
			<div class="{style.attributeChoice.shadow}"></div>
			<div class="{style.attributeChoice.shadowBottom}"></div>
			<g:FlowPanel styleName="{style.attributeChoice.choices}" ui:field="m_choices"></g:FlowPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>